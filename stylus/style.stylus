*
  margin 0
  padding 0
  
ul li
  list-style none
  
ul li a
  color rgb(78, 74, 80)
  display block
  font-family 'Roboto'
  line-height 40px
  text-align center
  text-decoration none
  
ul[btn-menu]
  background-color rgb(234, 232, 234)
  width 20%
 
ul[sub-menu]
  display none
  
ul li a:hover
  background-color rgb(29, 147, 211)
  color #fff
  
ul li a[active]
  background-color rgb(29, 147, 211)
  color #fff
  